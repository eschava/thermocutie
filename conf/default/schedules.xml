<Schedules>
    <Schedule name="default" type="week">
        <DaySet days="Mon-Fri" name="Workday">
            <Period mode="Home" start="06:00"/>
            <Period mode="Work" start="09:00"/>
            <Period mode="Home" start="18:00"/>
